# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                            @controllers.Application.index

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file                @controllers.Assets.versioned(path="/public", file: Asset)


GET        /user/getUserById            @controllers.user.getUserDetail
GET        /user/register               @controllers.user.register
GET        /user/updateLoginInfo        @controllers.user.updateLoginInfo
GET        /user/modifyUserInfo         @controllers.user.modifyUserInfo



GET       /consultant/create                       @controllers.consultant.creatConsultant
GET       /consultant/getConsultantInfo            @controllers.consultant.getConsultantInfo(userid:Long)




